# Environment Variables for Frontend
# Copy this file to .env and fill in your values

# Backend URL - Where your API is hosted
# FOR LOCAL DEVELOPMENT:
REACT_APP_BACKEND_URL=http://localhost:8001

# FOR PRODUCTION:
# Replace with your actual backend URL from your hosting provider
# Examples:
# Render: https://your-app-name.onrender.com
# Railway: https://your-backend-service.up.railway.app
# Heroku: https://your-app-name.herokuapp.com
# REACT_APP_BACKEND_URL=https://your-backend-url-here

# Build optimization (set to false for production)
GENERATE_SOURCEMAP=false

# WebSocket port for development (keep as is)
WDS_SOCKET_PORT=443